@model IEnumerable<ChannelEngineAssessment.Models.OrderViewModel>

@{
    ViewBag.Title = "Index";
}
<div><b>Table Format Data Displaying in ForEach</b><br /></div>
<table class="table table-bordered table-responsive table-hover">
    <tr>
        <th>Employee Id </th>
        <th>First Name </th>
        <th>Last Name </th>
        <th>Email</th>
        <th>Salary</th>
        <th>Company</th>
        <th>Department</th>
    </tr>
    @foreach (var d in Model)
    {
<tr>
    <td>@d.Id</td>
    <td>@d.ChannelName</td>
    <td>@d.ChannelOrderNo</td>
    <td>@d.TotalInclVat</td>
    <td>@d.TotalVat</td>
    <td>@d.CurrencyCode</td>
    <td>@d.Phone</td>
    <td>@d.Email</td>
    <td>@d.OrderDate</td>

</tr>

    }
</table>
